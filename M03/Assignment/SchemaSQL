CREATE TABLE players (
  player_id INTEGER PRIMARY KEY,
  first_name TEXT,
  last_name TEXT,
  age INTEGER,
  team_id INTEGER,
  FOREIGN KEY(team_id) REFERENCES teams(team_id)
  );

CREATE TABLE teams (
  team_id INTEGER PRIMARY KEY,
  name TEXT,
  location TEXT,
  coach TEXT
  );

